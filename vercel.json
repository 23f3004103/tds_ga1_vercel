{
  "version": 2,
  "headers": [
    {
      "source": "/api/latency(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "*"
        }
      ]
    }
  ],
  "builds": [
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/latency/?$",
      "dest": "api/latency.py"
    },
    {
      "src": "/api/latency/(.*)",
      "dest": "api/latency.py"
    }
  ]
}
